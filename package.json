{
  "name": "fuel-toolchain",
  "version": "0.3.0",
  "private": true,
  "description": "Install the Fuel toolchain",
  "main": "lib/main.js",
  "scripts": {
    "build": "rm -rf ./dist/* && ncc build src/main.ts --minify",
    "format": "prettier --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "lint": "tsc --noEmit && eslint 'src/**/*.ts' '__tests__/**/*.ts'",
    "watch": "rm -rf ./dist/* && ncc build src/main.ts --watch",
    "test": "jest -c jest.config.json --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FuelLabs/action-fuel-toolchain.git"
  },
  "keywords": [
    "actions",
    "sway",
    "fuel",
    "fuelup",
    "toolchain"
  ],
  "author": "Fuel Labs <contact@fuel.sh>",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^18.11.3",
    "@typescript-eslint/parser": "^5.40.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.25.0",
    "eslint-plugin-github": "^4.4.0",
    "eslint-plugin-jest": "^27.1.1",
    "jest": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.4"
  }
}
